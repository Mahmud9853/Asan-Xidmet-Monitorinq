@model Regulation

<main style="margin-top:50px; margin-bottom:50px;">
    <div class="container">

        <h1 style="font-family: 'Noto Sans';
        font-style: normal;
        font-weight: 700;
        font-size: 24px;
        line-height: 28px;
        padding-bottom: 20px;
        color: #383838;">
            @Model?.Title
        </h1>
        <p style="font-family: 'Noto Sans';
        font-style: normal;
        font-weight: 200;
        font-size: 14px;
        line-height: 28px;
        color: #383838;">   @Html.Raw(Model?.Description) </p>
    </div>

</main>
@model Legislation
@{
    ViewData["Title"] = "Detail";
}

<div class="container ">
    <h1 class="text-center mb-5">@Model.Title</h1>
    <div class="row d-flex justify-content-around mb-5 p-3">
        <div class="col-md-12">
            <span>@Html.Raw(Model.Svg)</span>
            <span>@Html.Raw(Model.Attachment)</span>
        </div>
      
    </div>
</div>
<a class="btn btn-outline-info mx-2 btn-rounded" asp-controller="Legislation" asp-action="Index">go Back</a>





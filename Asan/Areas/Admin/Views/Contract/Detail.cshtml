@model Contract

@{
    ViewData["Title"] = "Detail";
}
<div class="container ">
    <h1 class="text-center mb-5">@Model.Title</h1>
    <div class="row d-flex justify-content-around mb-5 p-3">

        <div class="col-md-6">
           @Html.Raw(Model.Svg)
        </div>
        <div class="col-md-6">
            <p>@Html.Raw(Model.Description)</p>

        </div>
    </div>
</div>
<a class="btn btn-outline-info mx-2 btn-rounded" asp-controller="Contract" asp-action="Index">go Back</a>



